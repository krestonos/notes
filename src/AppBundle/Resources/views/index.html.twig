{% extends 'base.html.twig' %}

{% block body %}
    <table class="table table-hover">
        <tr>
            <th>ID</th>
            <th>Note</th>
        </tr>
        {% for note in notes %}
            <tr>
                <td>{{ note.getId }}</td>
                <td>{{ note.getText }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}
