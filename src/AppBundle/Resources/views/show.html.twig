{% extends 'base.html.twig' %}
{% block body %}
    <table class="table table-hover">
        <tr>
            <th>ID</th>
            <th>Note text</th>
        </tr>
        <tr>
            <td>{{note.getId}}</td>
            <td>{{note.getText}}</td>
        </tr>
    </table>
    <a class="btn btn-primary" href="{{ path('allNotes') }}">To all notes</a>
    <a class="btn btn-danger" href="{{ path('deleteNote', {'id': note.getId}) }}">Delete</a>
{% endblock %}

